    "engines": {
        "node": ">=21.0.0",
        "npm": "use yarn instead",
        "pnpm": "use yarn instead",
        "yarn": ">=4.0.0"
    },
    "name": "ms0503-dev",
    "packageManager": "yarn@4.9.1",
    "private": true,
    "scripts": {
        "build": "react-router build",
        "clean": "rimraf build/",
        "deploy": "yarn run build && wrangler pages deploy",
        "dev": "react-router dev",
        "fontgen": "run-s fontgen:otf2ttf fontgen:subset fontgen:fixfontname",
        "fontgen:fixfontname": "patch -p1 <fix-font-name.patch",
        "fontgen:otf2ttf": "run-p 'fontgen:otf2ttf:*'",
        "fontgen:otf2ttf:bold": "fontforge otf2ttf.pe Bold",
        "fontgen:otf2ttf:regular": "fontforge otf2ttf.pe Regular",
        "fontgen:subset": "fontmin -t \"$(cat noto-sans-mono-cjk-jp-include.txt)\" fonts/NotoSansMonoCJKjp-*.ttf public/fonts",
        "lint": "run-p 'lint:lint:*'",
        "lint:fix": "run-p 'lint:fix:*'",
        "lint:fix:js": "eslint --cache --cache-location ./node_modules/.cache/eslint --fix .",
        "lint:fix:style": "stylelint app/**/*.css --fix",
        "lint:lint:js": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
        "lint:lint:style": "stylelint app/**/*.css",
        "preview": "yarn run build && wrangler pages dev",
        "start": "wrangler pages dev ./build/client",
        "typecheck": "yarn run typegen && tsc",
        "typegen": "react-router typegen && wrangler types"
    },
    "sideEffects": false,
    "type": "module"
}
