{
    "dependencies": {
        "drizzle-orm": "^0.42"
    },
    "devDependencies": {
        "@eslint/compat": "^1",
        "@eslint/eslintrc": "^3",
        "@types/node": "^22",
        "dotenv": "^16",
        "drizzle-kit": "^0.31",
        "eslint": "^9",
        "jiti": "^2",
        "ms0503-dev-eslint": "workspace:*",
        "npm-run-all": "^4",
        "rimraf": "^6",
        "ts-add-js-extension": "^1",
        "typescript": "^5",
        "wrangler": "^4"
    },
    "engines": {
        "node": ">=21.0.0",
        "npm": "use yarn instead",
        "pnpm": "use yarn instead",
        "yarn": ">=4.0.0"
    },
    "exports": {
        ".": {
            "@ms0503-dev/condition-ts": "./src/index.ts",
            "import": "./build/index.js",
            "types": "./build/index.d.ts"
        }
    },
    "name": "ms0503-dev-db",
    "packageManager": "yarn@4.9.1",
    "scripts": {
        "build": "tsc --noEmit false --outDir build/ && ts-add-js-extension --dir=build/ && tsc --declaration true --emitDeclarationOnly --noEmit false --outDir build/",
        "clean": "rimraf build/",
        "dev": ":",
        "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint/ .",
        "lint:fix": "eslint --cache --cache-location ./node_modules/.cache/eslint/ --fix .",
        "typecheck": "yarn run typegen && tsc",
        "typegen": "wrangler types"
    },
    "type": "module"
}
