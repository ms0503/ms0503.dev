{
    "$schema": "https://schema.tauri.app/config/2",
    "app": {
        "security": {
            "csp": null
        },
        "windows": [
            {
                "height": 600,
                "title": "ms0503-dev-editor",
                "width": 800
            }
        ]
    },
    "build": {
        "beforeBuildCommand": "yarn build",
        "beforeDevCommand": "yarn dev",
        "devUrl": "http://localhost:1420",
        "frontendDist": "../dist"
    },
    "bundle": {
        "active": true,
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ],
        "targets": "all"
    },
    "identifier": "dev.ms0503.ms0503-dev-editor",
    "productName": "ms0503-dev-editor",
    "version": "0.1.0"
}
